#
PROJECT_BRIEF          = "perlqt5 is a binding to Qt5 using smokeqt"

PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER         = @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@

INPUT                  = \
        @doxy_main_page@ \
        @PROJECT_SOURCE_DIR@/modules \
        @PROJECT_SOURCE_DIR@/src \
        @PROJECT_SOURCE_DIR@/smokegen \
        @PROJECT_BINARY_DIR@/smokeqt \
        @PROJECT_SOURCE_DIR@/common \
        @PROJECT_SOURCE_DIR@/targets

FILE_PATTERNS          = \
  *.h *.cc *.c \
  *.pl *.pm *.xs \


EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_METHODS  = YES
RECURSIVE              = YES
CREATE_SUBDIRS         = YES
EXCLUDE_SYMLINKS       = YES

#EXCLUDE                = */
#input buffer overflow, can't enlarge buffer because scanner uses REJECT
EXCLUDE_PATTERNS       = *.d \







GENERATE_HTML          = YES
#HTML_HEADER            = "@APIDOC_DIR@/html/header.html"
#HTML_FOOTER            = "@APIDOC_DIR@/html/footer.html"

USE_MDFILE_AS_MAINPAGE = @doxy_main_page@


GENERATE_LATEX         = @DOXYGEN_GENERATE_LATEX@
GENERATE_XML           = @DOXYGEN_GENERATE_XML@
GENERATE_MAN           = @DOXYGEN_GENERATE_MAN@
GENERATE_ECLIPSEHELP   = NO
GENERATE_LATEX         = NO

SEARCHENGINE           = YES

INCLUDE_PATH           = \
        /usr/include /usr/local/include \
	@SMOKE_QTGUI_INCLUDE_DIR@ \
/usr/lib/x86_64-linux-gnu/perl/5.30/CORE \
/usr/include/x86_64-linux-gnu/qt5/QtCore/5.12.5 \
/usr/include/x86_64-linux-gnu/qt5/QtCore/5.12.5/QtCore \
 @QT_INCLUDES@

# name=definition (no spaces). If the definition and the "=" are omitted, "=1"
# is assumed. To prevent a macro definition from being undefined via #undef or recursively expanded use the :=
#PREDEFINED             =



SOURCE_BROWSER         = YES
INLINE_SOURCES         = YES
REFERENCED_BY_RELATION = YES
REFERENCES_LINK_SOURCE = YES

#USE_HTAGS              = NO
#CLANG_ASSISTED_PARSING = NO

# TAGFILES = file1=loc1 "file2 = loc2" ...
GENERATE_TAGFILE       = perlqt5.tag


#@DX_COND_dot_TRUE@HAVE_DOT               = YES
HAVE_DOT               = @HAVE_DOT@
#DOT_PATH               = "@DOXYGEN_DOT_PATH@"
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
MAX_DOT_GRAPH_DEPTH    = 4
DOT_FONTSIZE           = 6
UML_LOOK               = YES
INTERACTIVE_SVG        = NO


